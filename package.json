{
  "name": "web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --disable-host-check --host 0.0.0.0",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "format": "prettier --write --config ./.prettierrc src/**/*{.ts,.js,.json,.scss,.html}",
    "eslint:fix": "eslint . --fix",
    "test": "ng test",
    "lint": "ng lint",
    "husky:install": "./node_modules/husky/lib/bin.js install",
    "husky:uninstall": "./node_modules/husky/lib/bin.js uninstall",
    "prepare": "npm run husky:install"
  },
  "private": true,
  "dependencies": {
    "@angular-eslint/eslint-plugin": "^13.2.1",
    "@angular/animations": "~13.3.3",
    "@angular/cdk": "^13.3.3",
    "@angular/common": "~13.3.3",
    "@angular/compiler": "~13.3.3",
    "@angular/core": "~13.3.3",
    "@angular/forms": "~13.3.3",
    "@angular/material": "^13.3.3",
    "@angular/platform-browser": "~13.3.3",
    "@angular/platform-browser-dynamic": "~13.3.3",
    "@angular/router": "~13.3.3",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.8.1",
    "class-transformer": "^0.5.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5",
    "tslib": "^2.3.0",
    "zone.js": "^0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.3",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "~13.3.3",
    "@angular/compiler-cli": "~13.3.3",
    "@types/jasmine": "^3.10.5",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.4",
    "jasmine-core": "^4.0.1",
    "karma": "^6.3.17",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^4.0.2",
    "karma-jasmine-html-reporter": "~1.7.0",
    "prettier": "^2.6.1",
    "typescript": "^4.6.3"
  }
